{
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "avoid",
  "singleQuote": true,
  "quoteProps": "consistent",
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "plugins": [
    "prettier-plugin-tailwindcss"
  ],
  "tailwindFunctions": [
    "classNames",
    "cn"
  ],
  "tailwindPreserveWhitespace": false,
  "tailwindPreserveDuplicates": false
}